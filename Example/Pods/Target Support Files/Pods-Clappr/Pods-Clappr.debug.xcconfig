GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Clappr" "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Clappr" -isystem "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_LDFLAGS = -ObjC -l"Pods-Clappr-Clappr"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods